{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "removeComments": false,
    "strict": true,
    "declaration": true,
    "composite": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "es2019",
    ],
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
